{
    "name": "valeriy-brunov/extensions",
    "description": "Набор полезных расширений: AJAX, Cursor.",
    "type": "cakephp-plugin",
    "home": "https://github.com/valeriy-brunov/extensions",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "4.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Extensions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Extensions\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
